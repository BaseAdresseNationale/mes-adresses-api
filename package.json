{
  "name": "@etalab/api-bal",
  "version": "0.0.1",
  "description": "API permettant la gestion de bases d’adresses à l’échelon local",
  "contributors": [
    "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
    "Théophile Merlière <theophile.merliere@data.gouv.fr>",
    "Guillaume Fay <guillaume.fay@beta.gouv.fr>",
    "Fabien Tafforeau <fabien.tafforeau@beta.gouv.fr>"
  ],
  "author": "Équipe Adresse <adresse@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nest build api && nest build cron",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "node dist/apps/api/main.js",
    "start:api": "node dist/apps/api/main.js",
    "start:cron": "node dist/apps/cron/main.js",
    "start:debug": "nest start --debug --watch",
    "dev": "nest start --watch",
    "dev:api": "nest start api --watch",
    "dev:cron": "nest start cron --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "yarn test:api && yarn test:cron",
    "test:api": "jest --config ./apps/api/test/jest-e2e.json",
    "test:cron": "jest --config ./apps/cron/test/jest-e2e.json",
    "update-cadastre": "node ./scripts/update-communes-cadastre.js",
    "push-staging": "./scripts/push-staging.sh",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:run": "yarn typeorm migration:run -- -d ./typeorm.config.ts",
    "typeorm:migration:down": "yarn typeorm migration:revert -- -d ./typeorm.config.ts",
    "generate:openapi:signalement": "openapi --input http://localhost:5005/api-json --output ./libs/shared/src/openapi-signalement"
  },
  "dependencies": {
    "@ban-team/adresses-util": "^0.9.0",
    "@ban-team/shared-data": "^1.4.0",
    "@ban-team/validateur-bal": "^2.21.0",
    "@etalab/decoupage-administratif": "^4.0.0",
    "@etalab/project-legal": "^0.6.0",
    "@mapbox/tilebelt": "^1.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/typeorm": "^10.0.2",
    "@testcontainers/postgresql": "^10.10.4",
    "@turf/turf": "^6.5.0",
    "@types/express": "^4.17.17",
    "@types/geojson-vt": "^3.2.3",
    "@types/multer": "^1.4.10",
    "axios": "^1.6.0",
    "axios-mock-adapter": "^1.22.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.3",
    "cache-manager": "^5.7.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "csv-write-stream": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "geojson-vt": "^3.2.0",
    "get-stream": "^6.0.1",
    "got": "11.8.5",
    "handlebars": "^4.7.8",
    "hasha": "^5.2.2",
    "http-errors": "^2.0.0",
    "into-stream": "^6.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.1.0",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.5",
    "pumpify": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-loader": "^9.4.3",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "vt-pbf": "^3.1.3",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cors": "^2.8.15",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ava": "^5.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nock": "^13.3.3",
    "nyc": "^15.1.0",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/lib/(.*)$": "<rootDir>/apps/api/src/lib/$1",
      "^@/modules/(.*)$": "<rootDir>/apps/api/src/modules/$1",
      "^@/shared/(.*)$": "<rootDir>/libs/shared/src/$1"
    },
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ]
  },
  "engines": {
    "node": "22"
  }
}